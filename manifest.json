{
    "manifest_version": 3,
    "name": "Youtube Notion",
    "description": "Google Extension to add Youtube Links to Notion",
    "version": "1.0",
    "icons": {
        "16": "assets/yt-16.png",
        "32": "assets/yt-32.png",
        "48": "assets/yt-48.png",
        "128": "assets/yt-128.png"
      },

    "permissions": ["activeTab", "scripting"], 

    "background": {
        "service_worker": "background.js"
      }
}